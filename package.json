{
  "name": "db-viewer-vscode-file",
  "displayName": "DB Viewer",
  "description": "test",
  "publisher": "Kiran Mantha",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "files": [
    "dist",
    "resources"
  ],
  "main": "./dist/extension.js",
  "icon": "resources/db.png",
  "author": "KiranMantha",
  "bugs": {
    "url": "https://github.com/KiranMantha/db-viewer/issues"
  },
  "homepage": "https://github.com/KiranMantha/db-viewer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KiranMantha/db-viewer.git"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "assets": [
    "resources"
  ],
  "extensionKind": [
    "workspace",
    "ui"
  ],
  "activationEvents": [
    "onCustomEditor:db-viewer.databaseEditor"
  ],
  "contributes": {
    "languages": [
      {
        "id": "sqlite",
        "extensions": [
          ".db",
          ".sqlite",
          ".sqlite3"
        ],
        "aliases": [
          "SQLite",
          "sqlite"
        ],
        "mimetypes": [
          "application/x-sqlite3"
        ]
      }
    ],
    "customEditors": [
      {
        "viewType": "db-viewer.databaseEditor",
        "displayName": "DB Viewer",
        "selector": [
          {
            "filenamePattern": "*.db"
          },
          {
            "filenamePattern": "*.sqlite"
          },
          {
            "filenamePattern": "*.sqlite3"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack && node esbuild.config.js",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map && node esbuild.config.js",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "build": "vsce package"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/d3": "^7.4.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "better-sqlite3": "^11.7.0",
    "d3": "^7.9.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.16.0",
    "preact": "^10.25.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {}
}
